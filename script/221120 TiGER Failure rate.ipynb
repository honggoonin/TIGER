    "\tresult_sum += nCr(n,i)*(bit_error_rate**i)*((1-bit_error_rate)**(n-i))\n",
    "final_result = 1 - result_sum\n",
    "print(f'bit_error_rate = log^{log(bit_error_rate,2)}')"
    "print(f'DFR with ECC = log^{log(final_result,2)}')"
