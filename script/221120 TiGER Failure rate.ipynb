    "class TiGER128(Scheme):\n",
    "    def __init__(self, n=512, q=256, hs=160, hr=160, p=128, k1=64, k2=64, B=1, key_size=128):\n",
    "        params = {\"n\": n, \"q\": q, \"p\": p, \"k1\": k1, \"k2\" : k2, \"hs\": hs, \"hr\": hr, \"B\" : B}\n",
    "        Scheme.__init__(self, prec=None, amplify=key_size, **params)\n",
    "    @cached_method\n",
    "    def _D(self):\n",
    "        params = self.params\n",
    "        prec = self.prec\n",
    "        n = params[\"n\"]\n",
    "        \n",
    "        s = RandomVariable.SparseCenteredUniform(params[\"B\"], params[\"hs\"], params[\"n\"], prec=prec)     \n",
    "        e = RandomVariable.RoundingError(params[\"q\"], params[\"p\"], prec=prec)\n",
    "\n",
    "        r = RandomVariable.SparseCenteredUniform(params[\"B\"], params[\"hr\"], params[\"n\"], prec=prec)    \n",
    "        e_1= RandomVariable.SparseCenteredUniform(params[\"B\"], 32, params[\"n\"], prec=prec)           \n",
    "        e_2= RandomVariable.SparseCenteredUniform(params[\"B\"], 32, params[\"n\"], prec=prec)    \n",
    "        \n",
    "        c_u = RandomVariable.RoundingError(params[\"q\"], params[\"k1\"], prec=prec)\n",
    "        c_v = RandomVariable.RoundingError(params[\"q\"], params[\"k2\"], prec=prec)     \n",
    "       \n",
    "        D = c_v + e_2 + RandomVariable.sum(e*r, n) + RandomVariable.sum(s * (e_1 + c_u), n)        \n",
    "        return D\n",
    "    \n",
    "Scheme128=TiGER128()\n",
    "DFR_wo_ECC=Scheme128()\n",
    "print(\"DFR without ECC : \", DFR_wo_ECC)"
   ]
