    "import math\n",
    "from mpmath import mp, log\n",
    "def factorial(k):\n",
    "    fact = 1\n",
    "    for i in range(1, k+1):\n",
    "        fact *= i\n",
    "    return fact\n",
    "def nCr(n, r):\n",
    "    return factorial(n) / (factorial(r) * factorial(n-r))"
   ]
